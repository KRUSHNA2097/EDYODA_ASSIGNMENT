   "source": [
    "# Write a Python program to reverse a string.\n",
    "\n",
    "\n",
    "\n",
    "# ï»¿Sample String : \"1234abcd\"\n",
    "\n",
    "# Expected Output : \"dcba4321\"\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "c36b970b",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter a string: \"1234abcd\"\n",
      "Revers string:  dcba4321\n"
     ]
    }
   ],
   "source": [
    "def rev_str(x):\n",
    "  return x[::-1]\n",
    "\n",
    "mystr = eval(input(\"Enter a string: \"))\n",
    "mystr = rev_str(mystr)\n",
    "\n",
    "print(\"Revers string: \",mystr)"
   ]
  },
